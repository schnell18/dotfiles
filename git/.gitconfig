[user]
	email = schnell18@gmail.com
	name = Justin Zhang
[core]
	editor = nvim
	autocrlf = input
[color]
	ui = true
[merge]
	tool = vimdiff
[push]
	default = simple
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
[credential]
	helper = store
[init]
	defaultBranch = master
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[gpg]
	program = gpg2
